<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.p.fridge20">

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".welcome.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".shoppingList.AddToShoppingList"
            android:parentActivityName=".shoppingList.ShoppingListActivity" />
        <activity
            android:name=".shoppingList.ShoppingListActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".review.ReviewActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".addition.AddingActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".productDetails.ProductDetails"
            android:parentActivityName=".review.ReviewActivity" />
        <activity android:name=".MainActivity"/>
    </application>

</manifest>